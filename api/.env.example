# Copy to .env.local and fill values for local development.
# The launcher script (scripts/run_local.sh) auto-loads .env.local then .env.

# Core
DATABASE_URL=postgresql://USER:PASSWORD@HOST:5432/DBNAME
AUTH_MODE=disabled
ETI360_API_KEY=replace-me

# Map generation
MAPBOX_ACCESS_KEY=pk.replace-me
MAPBOX_TRIPRISK_MAP_STYLE=mapbox://styles/dskimin/cmlhz9ta7002i01rh1p9q6be5

# Weather + location helpers (optional unless used)
GOOGLE_MAPS_API_KEY=replace-me
